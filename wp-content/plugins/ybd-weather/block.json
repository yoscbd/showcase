{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ourplugin/weather-api-block",
  "editorScript": "file:./build/index.js",
  "editorStyle": "file:./build/index.css",
  "script": "file:./build/frontend.js",
  "style": "file:./build/index.css",
  "title": "weather report block",
  "icon": "smiley",
  "category": "common",
  "supports": {
    "html": false,
    "multiple": false,
    "color": {
      "link": true,
      "gradients": false,
      "background": true
    }
  },
  "attributes": {
    "skyColor": {
      "type": "string",
      "default": "Blue"
    },
    "grassColor": {
      "type": "string",
      "default": "Green"
    },
    "selectedCountry": {
      "type": "string",
      "default": "none"
    },
    "backgroundColor": {
      "type": "string",
      "default": "#222"
    },
    "textColor": {
      "type": "string",
      "default": "#fff"
    }
  }
}